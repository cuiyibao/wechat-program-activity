/**index.wxss**/
.container {
  position: relative;
  width: 100%;
  height: 100%;
  padding: 30rpx;
  padding-top:60rpx;
  background-color: #f5f5f5;
  color: #939393;
  font-size: 36rpx;
}

.list{
  padding: 20rpx;
}
.list .list-item{
  display: flex;
  position: relative;
  flex-direction: row;
  margin: 20rpx 10rpx;
  width: 100%;
}
.list .list-item .list-el{
  padding-right: 10rpx;
  text-align: left;
  height: 64rpx;
  line-height: 64rpx;
  width: 168rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.list .list-item .list-rl{
  flex: 1;
  height: 64rpx;
  line-height: 64rpx;
}